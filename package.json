{
  "name": "mantine-minimal-next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-p --continue-on-error lint:*",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check .",
    "fix": "run-s --continue-on-error fix:eslint fix:prettier",
    "fix:eslint": "npx eslint --fix .",
    "fix:prettier": "prettier --write .",
    "prepare": "husky install",
    "codegen": "graphql-codegen --config lib/codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "@apollo/react-hooks": "^4.0.0",
    "@auth0/nextjs-auth0": "^3.0.1",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "6.0.0",
    "@mantine/hooks": "6.0.0",
    "@mantine/next": "6.0.0",
    "cross-fetch": "^4.0.0",
    "graphql": "^16.7.1",
    "next": "13.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/node": "18.14.4",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "dotenv": "^16.3.1",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "eslint-config-prettier": "^8.9.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "npm run fix:eslint",
    "*.{js,ts,tsx,json,md}": "npm run fix:prettier"
  }
}
